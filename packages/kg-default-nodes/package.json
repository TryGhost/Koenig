{
  "name": "@tryghost/kg-default-nodes",
  "version": "0.0.0",
  "repository": "https://github.com/TryGhost/Koenig/tree/main/packages/kg-default-nodes",
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "exports": {
    "require": "./cjs/index.cjs",
    "import": "./index.js"
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "prepare": "NODE_ENV=production yarn build",
    "pretest": "yarn build",
    "test:unit": "NODE_ENV=testing c8 --all --exclude cjs/  --check-coverage --reporter text --reporter cobertura mocha './test/**/*.test.js'",
    "test": "yarn test:unit",
    "lint:code": "eslint *.js lib/ --ext .js --cache",
    "lint": "yarn lint:code && yarn lint:test",
    "lint:test": "eslint -c test/.eslintrc.cjs test/ --ext .js --cache"
  },
  "files": [
    "index.js",
    "lib",
    "cjs"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-syntax-import-assertions": "^7.20.0",
    "c8": "7.12.0",
    "mocha": "10.1.0",
    "rollup": "^3.5.0",
    "should": "13.2.3",
    "sinon": "14.0.2"
  },
  "dependencies": {
    "jsdom": "^20.0.3",
    "lexical": "^0.6.0"
  }
}
